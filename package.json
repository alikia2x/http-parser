{
    "name": "@alikia/http-parser",
    "version": "1.0.0",
    "description": "A robust, production-ready HTTP/1.x request and response parser written in TypeScript",
    "type": "module",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.js"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "bun bun.build.ts",
        "test": "bun test",
        "test:coverage": "bun test --coverage",
        "prepublishOnly": "bun run build",
        "format": "bun biome format --write",
        "lint": "bun biome check",
        "lint:fix": "bun biome check --fix"
    },
    "keywords": [
        "http",
        "parser",
        "http-parser",
        "http1",
        "https",
        "request",
        "response",
        "browser",
        "nodejs",
        "deno",
        "bun",
        "workers"
    ],
    "author": "alikia",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/alikia2x/http-parser"
    },
    "bugs": {
        "url": "https://github.com/alikia2x/http-parser/issues"
    },
    "homepage": "https://github.com/alikia2x/http-parser",
    "peerDependencies": {},
    "devDependencies": {
        "@biomejs/biome": "2.3.8",
        "typescript": "^5.9.3"
    }
}
